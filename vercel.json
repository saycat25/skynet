{
  "name": "military-chat-command",
  "version": 1,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_MILITARY_MODE": "production",
    "NEXT_PUBLIC_ENCRYPTION_KEY": "AES-256-GCM"
  }
}